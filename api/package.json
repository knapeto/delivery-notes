    "odbc": "^2.4.4",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.4.1",
    "prisma-nestjs-graphql": "^14.6.1",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rrule": "^2.6.8",
    "rxjs": "^7.2.0",
    "ssh2": "^1.11.0",
    "ts-node": "^10.0.0",
    "type-graphql": "1.1.1",
    "typegraphql-nestjs": "^0.4.1",
    "typegraphql-prisma": "0.16.3"
