{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn prisma:generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "seed": "yarn job:seed",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "start": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "echo 'skip'",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "yarn prisma migrate dev",
    "job:deployDB": "yarn prisma migrate deploy",
    "job:resetDB": "yarn prisma migrate reset -f",
    "postinstall": "prisma generate",
    "job:seed": "ts-node --project ./tsconfig.json scripts/seed",
    "dev:magic": "yarn && yarn job:resetDB && yarn seed && yarn dev"
  },
  "dependencies": {
    "@apollo/federation": "0.29.0",
    "@google-cloud/storage": "^5.18.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "^0.0.4",
    "@nestjs/common": "^8.1.2",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.1.2",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.6",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/schedule": "^1.1.0",
    "@prisma/client": "~3.3.0",
    "@sendgrid/mail": "^7.6.0",
    "amqp-connection-manager": "^4.1.1",
    "amqplib": "^0.8.0",
    "apollo-server-express": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "chance": "^1.1.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.10.7",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.0.10",
    "graphql-scalars": "^1.9.3",
    "graphql-shield": "^7.5.0",
    "graphql-tools": "^7.0.4",
